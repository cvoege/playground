{
  "name": "mimir-frontend-v3",
  "version": "0.0.1",
  "description": "Mimir's new frontend",
  "scripts": {
    "watch": "NODE_ENV=development gulp",
    "build-dev": "NODE_ENV=development gulp build",
    "build-prod": "NODE_ENV=production gulp build",
    "serve": "pushstate-server ./dist 8000",
    "dev": "concurrently \"npm run watch \" \"npm run serve\"",
    "eslint": "eslint ./",
    "eslint-fix": "eslint --fix ./",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "check": "npm run test && npm run flow && npm run eslint"
  },
