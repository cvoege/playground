    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chartist": "^0.9.8",
    "concurrently": "^3.1.0",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
